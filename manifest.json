{
    "name": "GoodNews",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["deepai.min.js","jquery.min.js", "contentscript.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "declarativeContent",
        "storage"
      ],
      "browser_action": {
        "default_popup": "popup.html"
      },
      "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "content_security_policy": "script-src 'unsafe-eval' 'self' https://code.jquery.com/jquery-3.5.1.min.js https://cdnjs.deepai.org/deepai.min.js; object-src 'self' "
}